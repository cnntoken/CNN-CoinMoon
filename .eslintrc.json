{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true,
        "commonjs": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-native/all"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType":"module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "react-native/no-color-literals": 0,
        "react-native/no-inline-styles": 0,
        "react/display-name": 0,
        "react/prop-types": 0,
        "no-console": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "eqeqeq": [
            "error"
        ]
    },
    "globals": {
        "fetch": true
    }
}